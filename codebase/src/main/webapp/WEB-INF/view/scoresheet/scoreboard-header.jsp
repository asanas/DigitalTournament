<div class="row">
    <div class="col-lg-4 text-center">
        <h2 class="teamContainer " id="defendingTeamName">${defendingTeamName }</h2>
    </div>
    <div class="col-lg-4 text-center">
        <h3><span class="glyphicon glyphicon-time icon-big" style="padding:0 10px;"></span>STOPWATCH</h3>
    </div>
    <div class="col-lg-4 text-center">
        <h2 class="teamContainer " id="chasingTeamName">${chasingTeamName}</h2>
    </div>
</div>
<div class="row">
    <div class="col-lg-4 text-center">
        <h1 id="defendingTeamScore" class="teamscore">${defendingTeamScore }</h1>
    </div>
    <div class="col-lg-4 text-center" style="border: 1px solid #7DBBC1">
        <div class="clock clearfix" style="width: 100%;"></div>
    </div>
    <div class="col-lg-4 text-center">
        <h1 id="chasingTeamScore" class="teamscore">${chasingTeamScore }</h1>
    </div>
</div>